<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".activity.Myapp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/contactlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/contactlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyContact"
        tools:targetApi="31">

        <activity
            android:name=".activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name="demo.contact.app.activity.LoginActivity" />
        <activity android:name="demo.contact.app.activity.RegisterActivity" />
        <activity android:name="demo.contact.app.activity.MainContactActivity" />
        <activity android:name="demo.contact.app.activity.AddContactActivity" />
        <activity android:name="demo.contact.app.activity.EditContactActivity" />
        <activity android:name="demo.contact.app.activity.ContactDetailActivity" />
        <activity android:name="demo.contact.app.activity.UpdateProfileActivity" />
        <activity android:name="demo.contact.app.activity.ChangePasswordActivity" />
        <activity android:name="demo.contact.app.activity.FavouriteActivity" />

    </application>

</manifest>